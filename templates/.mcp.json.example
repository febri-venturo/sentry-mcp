{
  "mcpServers": {
    "sentry": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@sentry/mcp-server",
        "--access-token=YOUR_SENTRY_ACCESS_TOKEN_HERE",
        "--host=YOUR_SENTRY_HOST"
      ],
      "env": {
        "MCP_DISABLE_SKILLS": "seer"
      }
    }
  }
}
